{
    "name": "Truk Manager",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icon/icon.png"
    }
}
